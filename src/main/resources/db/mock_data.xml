<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">
<changeSet id="mock_data_1" author="popov">
    <loadUpdateData tableName="roles" primaryKey="id" file="db/mock_data/roles.csv">
        <column name="id" type="numeric"/>
    </loadUpdateData>
</changeSet>
    <changeSet id="mock_data_2" author="popov">
        <loadUpdateData tableName="positions" primaryKey="id" file="db/mock_data/positions.csv">
            <column name="id" type="numeric"/>
        </loadUpdateData>
    </changeSet>
</databaseChangeLog>